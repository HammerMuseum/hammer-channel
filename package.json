    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vuejs-accessibility": "^0.1.3",
    "expect": "^26.6.2",
    "file-loader": "^6.2.0",
    "imagemin-gifsicle": "^7.0.0",
